<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.1"
           xsi:type="MailApp"
           Version="1.0.0.0"
           ProviderName="Data#3"
           DefaultLocale="en-US">
  <Id>f09f2988-a534-4763-ac23-e927831741dd</Id>
  <DisplayName DefaultValue="HashFix on Send"/>
  <Description DefaultValue="Superscript # before 3 in the email body at send time."/>
  <IconUrl DefaultValue="https://markarobinson-pixel.github.io/hashfix/icon-16.png"/>
  <HighResolutionIconUrl DefaultValue="https://markarobinson-pixel.github.io/hashfix/icon-32.png"/>
  <SupportUrl DefaultValue="https://www.data3.com"/>
  <Hosts><Host Name="Mailbox"/></Hosts>
  <Requirements><Sets DefaultMinVersion="1.10"><Set Name="Mailbox"/></Sets></Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead"><DesktopSettings><SourceLocation DefaultValue="https://markarobinson-pixel.github.io/hashfix/functions.html"/></DesktopSettings></Form>
    <Form xsi:type="ItemEdit"><DesktopSettings><SourceLocation DefaultValue="https://markarobinson-pixel.github.io/hashfix/functions.html"/></DesktopSettings></Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" Version="1.0">
    <Requirements><bt:Sets DefaultMinVersion="1.10"><bt:Set Name="Mailbox"/></bt:Sets></Requirements>
    <Hosts>
      <Host xsi:type="MailHost">
        <Runtimes><Runtime resid="Taskpane.Url" lifetime="long" /></Runtimes>
        <ExtensionPoint xsi:type="Events">
          <Event Type="ItemSend" FunctionExecution="synchronous" FunctionName="onMessageSend"/>
        </ExtensionPoint>
      </Host>
    </Hosts>
    <Resources>
      <bt:Urls><bt:Url id="Taskpane.Url" DefaultValue="https://markarobinson-pixel.github.io/hashfix/functions.html"/></bt:Urls>
      <bt:Images>
        <bt:Image id="Icon16" DefaultValue="https://markarobinson-pixel.github.io/hashfix/icon-16.png"/>
        <bt:Image id="Icon32" DefaultValue="https://markarobinson-pixel.github.io/hashfix/icon-32.png"/>
      </bt:Images>
      <bt:ShortStrings><bt:String id="residDisplayName" DefaultValue="HashFix on Send"/></bt:ShortStrings>
      <bt:LongStrings><bt:String id="residDescription" DefaultValue="Superscript # before 3 when you send."/></bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
